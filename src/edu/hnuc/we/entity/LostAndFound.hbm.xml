<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="edu.hnuc.we.entity.LostAndFound" table="lostandfound" catalog="we4hnuc">
        <id name="laf_id" type="java.lang.Integer">
            <column name="laf_id"/>
            <generator class="increment"></generator>
        </id>
        <property name="laf_type" type="java.lang.Integer">
            <column name="laf_type" />
        </property>
        <property name="laf_stat" type="java.lang.Integer">
            <column name="laf_stat" />
        </property>
        <property name="laf_laftime" type="timestamp">
            <column name="laf_laftime" />
        </property>
        <property name="laf_sucName" type="java.lang.String">
            <column name="laf_sucName" />
        </property>
       	<property name="laf_sucStuId" type="java.lang.String">
            <column name="laf_sucStuId" />
        </property>
        <property name="laf_sucPhone" type="java.lang.String">
            <column name="laf_sucPhone" />
        </property>
        <property name="laf_pubtime" type="timestamp">
            <column name="laf_pubtime" />
        </property>
        <property name="laf_suctime" type="timestamp">
            <column name="laf_suctime" />
        </property>
        <property name="laf_name" type="java.lang.String">
            <column name="laf_name"/>
        </property>
        <property name="laf_stuid" type="java.lang.String">
            <column name="laf_stuid"/>
        </property>
        <property name="laf_phone" type="java.lang.String">
            <column name="laf_phone"/>
        </property>
        <property name="laf_qq" type="java.lang.String">
            <column name="laf_qq"/>
        </property>
        <property name="laf_wechat" type="java.lang.String">
            <column name="laf_wechat"/>
        </property>
        <property name="laf_detail" type="java.lang.String">
            <column name="laf_detail"/>
        </property>
        <property name="laf_pic" type="java.lang.String">
            <column name="laf_pic"/>
        </property>
        
    </class>
</hibernate-mapping>
