<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="edu.hnuc.we.entity.AdminConfig" table="adminconfig" catalog="we4hnuc">
        <id name="confId" type="java.lang.Integer">
            <column name="conf_id" />
            <generator class="increment" />
        </id>
        <property name="confKey" type="java.lang.String">
            <column name="conf_key" length="50" />
        </property>
        <property name="confValue" type="java.lang.String">
            <column name="conf_value" length="250" />
        </property>
    </class>
</hibernate-mapping>
